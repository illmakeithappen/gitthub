# .env.superset
# Environment variables for GitThub with Superset integration

# Database Configuration
POSTGRES_USER=gitthub
POSTGRES_PASSWORD=gitthub
POSTGRES_DB=gitthub
POSTGRES_MULTIPLE_DATABASES=gitthub,superset,analytics

# Superset Configuration
SUPERSET_SECRET_KEY=your-super-secret-key-change-this-in-production
SUPERSET_GUEST_TOKEN_SECRET=your-guest-token-secret-change-this
SUPERSET_ADMIN_USERNAME=admin
SUPERSET_ADMIN_PASSWORD=admin-change-this-password
SUPERSET_URL=http://localhost:8088

# JWT Configuration
JWT_SECRET=your-jwt-secret-for-auth

# AWS Configuration (optional)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=gitthub-data

# Redis Configuration
REDIS_URL=redis://redis:6379

# Environment
ENV=development

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8001
REACT_APP_SUPERSET_URL=http://localhost:8088