# AWS RDS PostgreSQL Database
AWS_RDS_ENDPOINT=your-database.region.rds.amazonaws.com
AWS_RDS_PORT=5432
AWS_RDS_DB_NAME=gitthub
AWS_RDS_USERNAME=gitthubadmin
AWS_RDS_PASSWORD=your-password-here

# AWS S3 Storage
AWS_ACCESS_KEY_ID=AKIA...your-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here
AWS_REGION=us-west-2
AWS_S3_BUCKET=gitthub-databank-123456

# JWT Secret (generate a random one)
JWT_SECRET_KEY=gitthub-super-secret-jwt-key-2024-production

# Environment
NODE_ENV=development
PYTHON_ENV=development